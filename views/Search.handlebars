<div class="text-center text-primary mt-2" style="font-size: xx-large; font-weight: 600;">Youtube to Mp3 Api (Admin
    Panel)</div>
<div class="text-center text-primary mt-5" style="font-size: x-large; font-weight: 600;">Search Page</div>
<div class="container w-25 mt-5">
    <form method="post">
        <h5 class="text-center">Search by:</h5>
        <input type="hidden" name="FormName" value="Search" />
        <div class="radio">
            <label><input type="radio" name="option" value="Name" checked> Name</label>
        </div>
        <div class="radio">
            <label><input type="radio" name="option" value="Email"> Email</label>
        </div>
        <div class="radio">
            <label><input type="radio" name="option" value="IP"> IP</label>
        </div>
        <div class="radio">
            <label><input type="radio" name="option" value="MaxRequests"> Max Requests</label>
        </div>
        <div class="form-group">
            <input type="text" class="form-control" placeholder="Value" required="required" name="value"
                value="{{value}}">
            <div class="custom-control custom-checkbox mt-2">
                <input type="checkbox" class="custom-control-input" id="defaultUnchecked" name="Exact">
                <label class="custom-control-label" for="defaultUnchecked">Exact Match</label>
            </div>
        </div>
        {{#if Err}}
        <div class="form-group">
            <div class="text text-danger" style="font-size: 15;">* {{Err}}</div>
        </div>
        {{/if}}
        <div class="form-group">
            <button type="submit" class="btn btn-primary btn-block">Search</button>
        </div>
    </form>
</div>





{{#if Found}}
<div class="text-center text-primary mt-5" style="font-size: x-large; font-weight: 600;">Search Results</div>
<div class="container mt-2 w-100 align-self-center text-center">
    <table class="table table-striped table-bordered" style="border:black 2px solid;">
        <thead class="thead-dark">
            <tr>
                <th scope="col">Name</th>
                <th scope="col">Email</th>
                <th scope="col">Last Used ID</th>
                <th scope="col">Max Requests</th>
                <th scope="col">Allowed IP</th>
                <th scope="col">Api Key</th>
                <th scope="col">Change Pass</th>
            </tr>
        </thead>
        <tbody>
            {{#each members}}
            <tr>
                <td>{{this.Name}}</td>
                <td>{{this.Email}}</td>
                <td>{{this.LastUsedID}}</td>
                <td>{{this.MaxRequests}}</td>
                <td><code>{{this.IP}}</code></td>
                <td><code>{{this.ApiKey}}</code></td>
                <td><a href="/adminpanel/SetPass/{{this._id}}">Click Here</a></td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>
{{/if}}